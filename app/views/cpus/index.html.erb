<p id="notice"><%= notice %></p>

<h1>Cpus</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cores</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cpus.each do |cpu| %>
      <tr>
        <td><%= cpu.name %></td>
        <td><%= cpu.cores %></td>
        <td><%= link_to 'Show', cpu %></td>
        <td><%= link_to 'Edit', edit_cpu_path(cpu) %></td>
        <td><%= link_to 'Destroy', cpu, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cpu', new_cpu_path %>
